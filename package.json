{
  "name": "apha-integration-bridge",
  "version": "0.0.0",
  "description": "apha-integration-bridge",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "compose:up": "docker compose -f docker-compose.dev.yml up -d",
    "compose:stop": "docker compose -f docker-compose.dev.yml down && docker compose -f docker-compose.dev.yml rm --force --volumes",
    "compose:rm": "npm run compose:stop && docker volume rm oracledb-data",
    "docker:dev": "NODE_ENV=development npm run server:watch",
    "dev": "dotenvx run --convention=nextjs -- npm run server:watch",
    "dev:debug": "NODE_ENV=development npm run server:debug",
    "format": "prettier --write \"**/*.{cjs,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md}\"",
    "git:pre-commit-hook": "npm run format:check && npm run lint",
    "postinstall": "npm run setup:husky",
    "lint": "eslint --cache --cache-strategy content \"**/*.{cjs,js}\"",
    "lint:fix": "npm run lint -- --fix",
    "postversion": "git add package.json package-lock.json && git commit -m $npm_package_version",
    "test": "node --no-experimental-require-module ./node_modules/jest/bin/jest.js --coverage --verbose --runInBand",
    "test:watch": "node --no-experimental-require-module ./node_modules/jest/bin/jest.js --watch",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,json --legacy-watch ./src",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,json --legacy-watch ./src",
    "start": "NODE_ENV=production node --use-strict .",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@defra/hapi-tracing": "1.0.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.4.0",
    "@hapi/inert": "7.1.0",
    "@hapi/vision": "7.0.3",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.60.1",
    "@opentelemetry/exporter-metrics-otlp-http": "0.202.0",
    "@opentelemetry/instrumentation-hapi": "0.49.0",
    "@opentelemetry/sdk-metrics": "2.0.1",
    "@opentelemetry/sdk-node": "0.202.0",
    "@opentelemetry/sdk-trace-base": "2.0.1",
    "async-retry": "1.3.3",
    "aws-embedded-metrics": "4.2.0",
    "aws4": "1.13.2",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "glob": "11.0.2",
    "global-agent": "3.0.0",
    "hapi-pino": "12.1.0",
    "hapi-pulse": "3.0.1",
    "hapi-swagger": "17.3.2",
    "https-proxy-agent": "7.0.6",
    "joi": "17.13.3",
    "knex": "3.1.0",
    "mongo-locks": "3.0.2",
    "mongodb": "6.17.0",
    "node-fetch": "3.3.2",
    "oracledb": "6.8.0",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "undici": "7.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@babel/eslint-parser": "7.27.5",
    "@babel/plugin-transform-explicit-resource-management": "7.27.5",
    "@babel/preset-env": "7.27.2",
    "@dotenvx/dotenvx": "1.44.2",
    "@opentelemetry/exporter-trace-otlp-proto": "0.202.0",
    "@shelf/jest-mongodb": "5.2.2",
    "@types/async-retry": "1.4.9",
    "autoprefixer": "10.4.21",
    "babel-jest": "29.7.0",
    "babel-plugin-transform-import-meta": "2.3.3",
    "dotenv": "16.5.0",
    "eslint": "9.28.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "neostandard": "0.12.1",
    "nodemon": "3.1.10",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "testcontainers": "11.0.2"
  }
}
